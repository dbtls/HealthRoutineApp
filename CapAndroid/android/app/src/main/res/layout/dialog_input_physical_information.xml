<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="300dp"
    android:layout_height="400dp"
    android:padding="30dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">

    <TextView
        android:id="@+id/dialog_input_physical_information_textview_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/notosans"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="@string/dialog_input_physical_information_title"
        android:textColor="@color/blue"
        android:textSize="@dimen/text_large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/dialog_input_physical_information_layout_content"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/dialog_input_user_information_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/dialog_input_physical_information_textview_title">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/dialog_input_user_information_layout_height"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/dialog_input_physical_information_layout_weight"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/dialog_input_user_information_textView_height"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosans_bold"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="키"
                android:textAppearance="@style/text_input_name"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/dialog_input_user_information_edittext_height"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/dialog_input_user_information_edittext_height"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:digits="0123456789"
                android:inputType="number"
                android:text=""
                android:textAppearance="@style/text_input_value"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/dialog_input_user_information_textview_height_unit"
                app:layout_constraintStart_toEndOf="@+id/dialog_input_user_information_textView_height"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/dialog_input_user_information_textview_height_unit"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosans_bold"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="cm"
                android:textAppearance="@style/text_input_unit"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/dialog_input_user_information_edittext_height"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/dialog_input_physical_information_layout_weight"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@id/dialog_input_physical_information_layout_muscle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dialog_input_user_information_layout_height">

            <TextView
                android:id="@+id/dialog_input_physical_information_textView_weight"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosans_bold"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="몸무게"
                android:textAppearance="@style/text_input_name"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/dialog_input_physical_information_edittext_user_weight"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/dialog_input_physical_information_edittext_user_weight"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:digits="0123456789"
                android:inputType="number"
                android:text=""
                android:textAppearance="@style/text_input_value"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/dialog_input_physical_information_textview_weight_unit"
                app:layout_constraintStart_toEndOf="@+id/dialog_input_physical_information_textView_weight"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/dialog_input_physical_information_textview_weight_unit"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosans_bold"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="kg"
                android:textAppearance="@style/text_input_unit"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/dialog_input_physical_information_edittext_user_weight"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/dialog_input_physical_information_layout_muscle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@id/dialog_input_physical_information_layout_fat"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/dialog_input_physical_information_layout_weight">

            <TextView
                android:id="@+id/dialog_input_physical_information_textView_muscle"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosans_bold"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="골격근량"
                android:textAppearance="@style/text_input_name"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/dialog_input_physical_information_edittext_user_muscle"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/dialog_input_physical_information_edittext_user_muscle"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:digits="0123456789"
                android:inputType="number"
                android:text=""
                android:textAppearance="@style/text_input_value"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/dialog_input_physical_information_textview_muscle_unit"
                app:layout_constraintStart_toEndOf="@+id/dialog_input_physical_information_textView_muscle"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/dialog_input_physical_information_textview_muscle_unit"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="kg"
                android:textAppearance="@style/text_input_unit"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/dialog_input_physical_information_edittext_user_muscle"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/dialog_input_physical_information_layout_fat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/dialog_input_physical_information_layout_muscle">

            <TextView
                android:id="@+id/dialog_input_user_information_textView_fat"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosans_bold"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="체지방량"
                android:textAppearance="@style/text_input_name"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/dialog_input_user_information_edittext_user_fat"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/dialog_input_user_information_edittext_user_fat"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:digits="0123456789"
                android:inputType="number"
                android:text=""
                android:textAppearance="@style/text_input_value"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/dialog_input_user_information_textview_fat_unit"
                app:layout_constraintStart_toEndOf="@+id/dialog_input_user_information_textView_fat"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/dialog_input_user_information_textview_fat_unit"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosans_bold"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="kg"
                android:textAppearance="@style/text_input_unit"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/dialog_input_user_information_edittext_user_fat"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/dialog_input_user_information_button"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/button_yes"
        android:text="확인"
        android:textAppearance="@style/text_button_yes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/dialog_input_physical_information_layout_content" />

</androidx.constraintlayout.widget.ConstraintLayout>